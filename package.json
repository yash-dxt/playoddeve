{
  "scripts": {
    "test": "go install github.com/joho/godotenv/cmd/godotenv@latest && godotenv -f ./.env.test go test ./...",
    "deploy:local": "serverless deploy --aws-profile local",
    "deploy:staging": "serverless deploy --aws-profile staging",
    "create-domain": "serverless create_domain --aws-profile prod",
    "deploy:prod": "serverless deploy --aws-profile prod",
    "deploy-function:prod": "serverless deploy function --function api --aws-profile prod",
    "download-go": "bash < <(curl -s -S -L https://raw.githubusercontent.com/moovweb/gvm/master/binscripts/gvm-installer)",
    "extract-go": "gvm install go1.18.1",
    "set-go": "gvm use go1.18.1",
    "setup": "npm run download-go && npm run extract-go && npm run set-go && go version"
  },
  "devDependencies": {
    "serverless-domain-manager": "^6.2.0",
    "serverless-go-plugin": "^2.2.0",
    "serverless-vpc-plugin": "^1.0.6"
  }
}